{
    "typeAcquisition": {
        "include": [
            "jest"
        ]
    },
    "compilerOptions": {
        "lib": ["es2023"],
        "module": "node16",
        "target": "es2022",
        "checkJs": true,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "moduleResolution": "node16"
      },
    "include": ["./**/*"],
    "exclude": ["node_modules/**/*"]
}